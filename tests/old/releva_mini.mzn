include "globals.mzn";
set of int: uni = {1,2,3,4,5,6};
set of int: d = {1,2,3};
set of int: f = {3,4,5};
set of int: s = {2,3,4};
int: n = 4;
array[1..n] of var uni: sequence;
constraint alldifferent(sequence);
constraint sequence[1] in d;
constraint sequence[2] in d;
constraint sequence[3] in f;
constraint sequence[4] in f;
constraint sum(i in 1..n)(bool2int(sequence[i] in s)) >= 2 /\ sum(i in 1..n)(bool2int(sequence[i] in s)) <= 2;
solve satisfy;